<script>
function Dog(name) {
	this.name = name;
}
function makeDog(name) {
	return new Dog(name);
}

var huan = (function () {
	return makeDog('huanhuan');
})();
var hua = makeDog('hua');
console.log(huan);
console.log(hua);
</script>
